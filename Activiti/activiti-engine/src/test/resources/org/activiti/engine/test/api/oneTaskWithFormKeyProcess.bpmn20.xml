<?xml version="1.0" encoding="UTF-8"?>
<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:activiti="http://activiti.org/bpmn"
  targetNamespace="Examples">

  <process id="oneTaskWithFormProcess" name="The One Task Process">
    <documentation>This is a process for testing purposes</documentation>

    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="theTask" />
    <userTask id="theTask" name="my task" activiti:formKey="myFormKey" activiti:assignee="myAssignee" activiti:owner="myOwner"
    	activiti:priority="60" activiti:category="myCategory" activiti:dueDate="2021-01-01" />
    <sequenceFlow id="flow2" sourceRef="theTask" targetRef="theEnd" />
    <endEvent id="theEnd" />

  </process>

</definitions>
