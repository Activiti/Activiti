<definitions
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:activiti="http://activiti.org/bpmn"
  targetNamespace="Examples">

  <process id="twoTasksProcess">
  
    <startEvent id="start" />
    <sequenceFlow id="flow1" sourceRef="start" targetRef="userTask1" />
    <userTask id="userTask1" name="User Task1" />    
    <sequenceFlow id="flow2" sourceRef="userTask1" targetRef="userTask2" />
    <userTask id="userTask2" name="User Task2" />    
    <sequenceFlow id="flow3" sourceRef="userTask2" targetRef="end" />
    <endEvent id="end" />
    
  </process>

</definitions>