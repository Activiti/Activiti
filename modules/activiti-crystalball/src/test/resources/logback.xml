<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="CA" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-5p %c %x - %m%n</pattern>
    </encoder>
  </appender>
  <logger name="org.activiti.crystalball.simulator.LogTimeEventHandler" level="INFO"/>
  <logger name="org.activiti.crystalball" level="TRACE"/>
  <root level="WARN">
    <appender-ref ref="CA"/>
  </root>
</configuration>